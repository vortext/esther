root      ::= obj
quote     ::= "\""

obj       ::= ("{" quote "imagination" quote ": " string ","
                   quote "emoji" quote ": " emoji ","
                   quote "keywords" quote ": " kwarray ","
                   quote "message" quote ": " string "}")

string ::=
